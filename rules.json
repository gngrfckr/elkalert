{
    "rules":
        [
            {
                "name":"ERROR level alert",
                "index":"ewp-log-apps-*",
                "query":{
                    "bool": {
                        "must": [
                          {
                            "match": {
                              "level": "ERROR"
                            }
                          },
                          {
                            "range": {
                              "@timestamp": {
                                "gte": "now-2m",
                                "lte": "now"
                              }
                            }
                          }
                        ]
                      }
                },
                "interval":{
                  "minutes": "1"
                }
            },
            {
              "name":"WARN level alert",
              "index":"ewp-log-apps-*",
              "query":{
                  "bool": {
                      "must": [
                        {
                          "match": {
                            "level": "WARN"
                          }
                        },
                        {
                          "range": {
                            "@timestamp": {
                              "gte": "now-3m",
                              "lte": "now"
                            }
                          }
                        }
                      ]
                    }
              },
              "interval":{
                "minutes": "3"
              }
          }
        ]
}